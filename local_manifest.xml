      <copyfile src="core/root.mk" dest="Makefile" />
     </project>

     <!-- Device Folders, Kernel & Proprietary BLOBS -->
     <project path="vendor/huawei" name="CyanogenModu8800pro/android_proprietary_vendor_huawei" remote="github" revision="cm-11.0"/>
